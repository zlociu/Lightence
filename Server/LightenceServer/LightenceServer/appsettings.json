{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Token": {
    "Key": "<JWTokenKey>",
    "Issuer": "lightence.net",
    "Audience": "lightence.net",
    "Lifetime": "60"
  },
  "Smtp": {
    "Mail": "lightence@lightence.net",
    "Password": "<MailPassword>"
  },
  "ConnectionStrings": {
    "AzureDB": "<DBConnectionString>"
  },
  "SignalR": {
    "Key": "<SignalRConnectionStringWithSubscriptionKey>"
  },
  "CognitiveServices": {
    "Endpoint": "<CognitiveServicesEndpoint>",
    "SubscriptionKey": "<SubscriptionKey>"
  },
  "AllowedHosts": "*",
  "Azure": {
    "SignalR": {
      "Enabled": "true"
    }
  }
}